        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".practice_test_activity.PracticeTest" />
        <activity android:name=".create_study_group.CreateStudyGroupActivity" />
        <activity android:name=".scheduler_activity.Scheduler" />
        <activity android:name=".find_study_group_activity.FindStudyGroup" />
        <activity android:name=".study_tracker_activity.StudyTracker"></activity>
    </application>

</manifest>